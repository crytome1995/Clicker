{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/src/index.js",
      "runtimeExecutable": "nodemon",
      "restart": true,
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["--inspect=5858"],
      "port": 5858,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
